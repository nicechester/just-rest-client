{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Just REST Client",
  "version": "0.1.0",
  "identifier": "com.chester.just-rest-client",
  "build": {
    "frontendDist": "../web",
    "devUrl": "http://localhost:9001"
  },
  "app": {
    "windows": [
      {
        "title": "Just REST Client",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
